<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.happy.user">

	<select id="selEntireUserInfo" resultType="com.happy.user.domain.resAllUserAccountDto">
		select * from user
	</select>
	
	<insert id="userRegist" parameterType="com.happy.user.domain.reqRegUserAccountDto">
		INSERT INTO user
		VALUES (#{userMngtNo}, #{emailAdr}, #{userPwd}, #{userGender}, #{userBirdD})
	</insert>

</mapper>